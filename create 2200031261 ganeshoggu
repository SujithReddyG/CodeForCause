#include<stdio.h>
int main()
{
    int m,n,sum=0;
    printf("enter m,n");
    scanf("%d%d",&m,&n);
    int a[m][n],i,j;
    printf("enter matrix elements");
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            sum=sum+a[i][j];
        }

    }
    printf("%d",sum);
}


